{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Carp": {
    "Consul": {
      "Host": "localhost",
      "Port": 8500,
      "Protocol": "http",
      "Token": "",
      "Interval": 2000
    },
    "Routes": [
      {
        "Descriptions": "简单的例子",
        "ServiceName": "DemoService",
        "LoadBalancerOptions": "RoundRobin",
        "PathTemplate": "basics/{**catch-all}"
      },
      {
        "Descriptions": "App服务",
        "ServiceName": "AppService",
        "LoadBalancerOptions": "RoundRobin",
        "PathTemplate": "app/{**catch-all}"
      } 
    ]
  },
  "AllowedHosts": "*"
}